{
  "source": {
    "include": [
      "src/components",
      "src/pages",
      "src/routes",
      "src/utils",
      "src/services"
    ],
    "includePattern": ".+\\.jsx?$",
    "excludePattern": "(node_modules/|dist/)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/docdash"
  },
  "plugins": ["plugins/markdown"]
}
