{
  "source": {
    "include": [
      "src/components",
      "src/pages",
      "src/routes",
      "src/utils",
      "src/services"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["node_modules/better-docs/component"],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "templates": {
    "better-docs": {
      "name": "OleoControl"
    }
  }
}